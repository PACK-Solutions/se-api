# Environment variables for Suite Epargne API
# Copy this file to .env and update with your actual values

# Datadog Configuration
DD_API_KEY=your-datadog-api-key-here
# Deployment environment for Datadog and tags (e.g., local, dev, prod)
DD_ENV=local
# Application version reported to Datadog (e.g., 0.0.1)
DD_VERSION=0.0.1

DATABASE_URL=jdbc:postgresql://localhost:5432/suite_epargne
DATABASE_USER=postgres
DATABASE_PASSWORD=postgres
